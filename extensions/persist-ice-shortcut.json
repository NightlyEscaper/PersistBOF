        }
    ],
    "arguments": [
        {
            "name": "Action",
            "desc": "Install = 0 / Unsintall = 1.",
            "type": "int",
            "optional": false
        },
        {
            "name": "FolderName",
            "desc": "Full Folder name.",
            "type": "wstring",
            "optional": false
        },
        {
            "name": "EXEName",
            "desc": "EXE that you wanna hijack.",
            "type": "wstring",
            "optional": false
        }
    ]
